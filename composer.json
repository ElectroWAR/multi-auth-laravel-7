{
  "name": "bmatovu/multi-auth",
  "description": "Laravel Multi Authentication",
  "homepage": "https://bmatovu.com/packages/multi-auth",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "package",
    "multi",
    "authentication"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "https://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/auth": "5.3.*",
    "illuminate/routing": "5.3.*",
    "illuminate/console": "5.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "orchestra/testbench": "~3.3.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\MultiAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\MultiAuth\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\MultiAuth\\MultiAuthServiceProvider"
      ],
      "aliases": {
        "MultiAuth": "Bmatovu\\MultiAuth\\MultiAuth"
      }
    }
  }
}
